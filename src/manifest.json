{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "homepage_url": "https://github.com/Creling/Yet-Another-LinkAce-Extension",
  "description": "A Browser Extension for LinkAce",
  "default_locale": "en",
  "permissions": [
    "*://*/*",
    "tabs",
    "storage"
  ],
  "icons": {
    "180": "icons/180.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "__MSG_extName__"
  }
}
